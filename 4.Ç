#include <stdio.h>

int main() {
    // Write C code here
    printf("Hello world");
    int num1=8;
    int num2=4;
    int num3=2;
    int soma=num1+num2+num3;
printf("a soma é:%i", soma);
int subtracao=num1-num2-num3;
printf("a subtracao é:%i", subtracao);
int multiplicacao= num1*num2*num3;
printf("a multiplicao é:%i", multiplicacao);
    return 0;
}
